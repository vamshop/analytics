{
    "name": "vamshop/analytics",
    "type": "cakephp-plugin",
    "description": "VamShop Google Analytics Plugin",
    "keywords": [
        "cakephp",
        "vamshop",
        "google analytics"
    ],
    "homepage": "https://github.com/vamshop/analytics",
    "license": "MIT",
    "authors": [
        {
            "name": "Rachman Chavik"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2",
        "vamshop/vamshop-core": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Analytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Analytics\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
